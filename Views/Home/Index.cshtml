@model TextbooksListViewModel

<div class="text-center">
    <h1 class="display-4">Textbook-Finder</h1>

</div>

<div class="container">
    <div class="row">
        <div class="col">
            <a asp-action="Index" asp-controller="Textbooks">Textbooks</a>
        </div>
        <div class="col">
            <a asp-action="Index" asp-controller="TextbookPublishers">TextbookPlublishers</a>
        </div>
    </div>
</div>


<h3> List of Textbooks </h3>

 @foreach (var t in Model.Textbooks)
 { 
    <partial name="TextbookSummary" model="t" />
 }

<div page-model="@Model.PagingInfo" page-action="Index" page-classes-enabled="true"
    page-class="btn" page-class-normal="btn-outline-dark"
    page-class-selected="btn-secondary" page-url-category="@Model.CurrentCategory">
</div>




